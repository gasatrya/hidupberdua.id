{
  "name": "hidupberdua",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^1.0.0",
    "@tailwindcss/typography": "^0.5.7",
    "astro": "^1.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.4"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "singleQuote": false
        }
      },
      {
        "files": [
          "*.css",
          "*.scss"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
