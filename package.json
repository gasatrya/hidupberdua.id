{
  "name": "hidupberdua.id",
  "version": "1.0.0",
  "description": "hidupberdua.id blog.",
  "main": "index.js",
  "scripts": {
    "dev:eleventy": "eleventy --serve --incremental",
    "dev:sass": "sass --watch src/_assets/scss/style.scss dist/css/style.css",
    "dev": "cross-env ELEVENTY_ENV=development run-p clean dev:*",
    "build:sass": "sass --no-source-map --style=compressed src/_assets/scss/style.scss dist/css/style.css",
    "build:eleventy": "eleventy",
    "build": "cross-env ELEVENTY_ENV=production run-s build:*",
    "clean": "rimraf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gasatrya/hidupberdua.id.git"
  },
  "author": "Ga Satrya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gasatrya/hidupberdua.id/issues"
  },
  "homepage": "https://github.com/gasatrya/hidupberdua.id#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.15",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0",
    "luxon": "^3.0.3",
    "markdown-it": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.2",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "sanitize.css": "^13.0.0"
